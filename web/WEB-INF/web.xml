<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>services.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>services.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>services.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>services.ChangePassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddEvent</servlet-name>
        <servlet-class>services.AddEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllEvents</servlet-name>
        <servlet-class>services.GetAllEvents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllUsers</servlet-name>
        <servlet-class>services.GetAllUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUsersForEvent</servlet-name>
        <servlet-class>services.GetUsersForEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAvailableUsersForEvent</servlet-name>
        <servlet-class>rowmappers.GetAvailableUsersForEvent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/ChangePassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddEvent</servlet-name>
        <url-pattern>/AddEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllEvents</servlet-name>
        <url-pattern>/GetAllEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllUsers</servlet-name>
        <url-pattern>/GetAllUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUsersForEvent</servlet-name>
        <url-pattern>/GetUsersForEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAvailableUsersForEvent</servlet-name>
        <url-pattern>/GetAvailableUsersForEvent</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
